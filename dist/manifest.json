 {
  "manifest_version": 2,
  "name": "Subadub - Jnani's Simplified 中文 Version",
  "description" : "Enhanced Netflix subtitles for foreign language study - Jnani's version",
  "version": "0.1.5.6",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }, 
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "run_at": "document_end",
      "js": ["content_script.js"]
    }
  ]
 }
